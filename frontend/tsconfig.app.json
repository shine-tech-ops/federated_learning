{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/layouts/components/*.vue",
    "env.d.ts",
    "src/auto-import.d.ts",
    "src/components/**/*.vue",
    "src/components/*.vue",
    "src/**/*.ts",
    "src/**/*.vue",
    "src/views/**/**/*.vue",
    "src/*.vue",
    "src/",
    "src/types/*.d.ts",
    "mock/*.ts",
    "components.d.ts"
  ],
  "exclude": ["node_modules", "dist", "src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "noEmit": true,
    "baseUrl": "./",
    "jsx": "preserve",
    "paths": {
      "@": ["src"],
      "@/*": ["src/*"]
    }
  }
}
